{
  "name": "mock-async-storage",
  "title": "Jest Mock AsyncStorage for react-native",
  "version": "1.0.0",
  "description": "Its a mock of react-native AsyncStorage for jest tests",
  "main": "lib/index.js",
  "keywords": [
    "jest",
    "react-native",
    "mock",
    "async-storage"
  ],
  "editions": [
    {
      "description": "Source + ESNext + Import + Flow",
      "entry": "src/index.js",
      "syntaxes": [
        "javascript",
        "esnext",
        "import",
        "arrows",
        "const",
        "let",
        "destructuring",
        "async"
      ]
    },
    {
      "description": "Babel Compiled + ES2015 + Require",
      "entry": "lib/index.js",
      "syntaxes": [
        "javascript",
        "es2015",
        "require"
      ]
    }
  ],
  "author": "Metál Ádám <devmetal91@gmail.com> (https://github.com/devmetal)",
  "repository": {
    "type": "git",
    "url": "https://github.com/devmetal/mock-async-storage.git"
  },
  "badges": {
    "list": [
      "travisci"
    ]
  },
  "license": "MIT",
  "scripts": {
    "tdd": "jest --watch",
    "lint": "eslint src",
    "test:src": "jest",
    "test": "npm run lint && npm run test:src",
    "build": "babel src --out-dir lib --ignore spec.js",
    "projectz": "projectz compile",
    "compile": "npm run lint && npm run build && npm run projectz"
  },
  "engines": {
    "node": ">=6.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-node6": "^11.0.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-import": "^2.2.0",
    "flow-bin": "^0.37.4",
    "jest": "^18.0.0"
  },
  "dependencies": {
    "deepmerge": "^1.3.1"
  },
  "maintainers": [],
  "contributors": [],
  "bugs": {
    "url": "https://github.com/devmetal/mock-async-storage/issues"
  }
}
